User
	id
	nombre
	apellido
	user
	pass
	status
	privileges
	created_at

publicaciones
	id
	id_usuario: para indicar al autor de la publicacion
	bueno
	regular
	malo
	denunciada
	estado: activa o eliminada
	tipo: meme o frase
	fecha: fecha en que se hizo la publicaci√≥n (datetime)

frases
	id
	id_publicacion
	frase
	estado: 1 activo o 0 eliminado
	tipo: f para que la critiquen como frase, m para que la critiquen como meme

Imagenes
	id
	id_publicacion
	nombre

memes
	id
	id_publicacion

meme_frase
	id
	id_meme

mem_imagen
	id
	id_meme

criticas
	id
	id_usuario
	id_publicacion
	cod_critica: b: bueno n:normal m:malo d:denunciado

observacion
	id
	id_critica
	observacion
